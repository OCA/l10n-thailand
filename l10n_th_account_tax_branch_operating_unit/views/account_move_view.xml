<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="view_move_form" model="ir.ui.view">
        <field name="name">account.move.form</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account_multi_branch_company.view_move_form" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='branch_id']" position="before">
                <field name="domain_branch_ids" widget="many2many_tags" invisible="1" />
            </xpath>
            <xpath expr="//field[@name='branch_id']" position="attributes">
                <attribute
                    name="domain"
                >[('company_id', '=', company_id), ('id', 'in', domain_branch_ids or [])]</attribute>
            </xpath>
        </field>
    </record>
</odoo>
