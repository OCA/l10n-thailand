<odoo>
    <record id="res_config_settings_view_form" model="ir.ui.view">
        <field name="name">res.config.settings.view.form.inherit.account</field>
        <field name="model">res.config.settings</field>
        <field name="priority" eval="40"/>
        <field name="inherit_id" ref="account.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//div[@id='print_vendor_checks_setting_container']" position="inside">
                <div class="col-12 col-lg-6 o_setting_box" id="bbl_ewht" groups="account.group_account_user">
                    <div class="o_setting_left_pane">
                        <field name="bbl_config_ewht"/>
                    </div>
                    <div class="o_setting_right_pane">
                        <label string="BBL Bank Payment Export - eWHT" for="bbl_config_ewht"/>
                        <div class="text-muted" id="payment_bank_bbl_ewht">
                            Payment Export BBL with eWHT.
                        </div>
                    </div>
                </div>
            </xpath>
        </field>
    </record>
</odoo>
