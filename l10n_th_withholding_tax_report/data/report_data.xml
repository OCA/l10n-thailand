<odoo>
    <!-- HMTL REPORTS -->
    <report
        id="action_withholding_tax_report_html"
        model="withholding.tax.report"
        string="Withholding Tax Report (HTML)"
        report_type="qweb-html"
        name="l10n_th_withholding_tax_report.report_withholding_tax_html"
        file="l10n_th_withholding_tax_report.report_withholding_tax_html"
    />
    <!-- PDF REPORTS -->
    <report
        id="action_withholding_tax_report_qweb"
        model="withholding.tax.report"
        string="Withholding Tax Report (PDF)"
        report_type="qweb-pdf"
        name="l10n_th_withholding_tax_report.report_withholding_tax_qweb"
        file="l10n_th_withholding_tax_report.report_withholding_tax_qweb"
        paperformat='l10n_th_withholding_tax_report.report_qweb_paperformat'
        print_report_name="'WT-%s-%s' % (object.format_pnd(object.income_tax_form), object.format_date())"
    />
    <!-- TEXT REPORTS -->
    <report
        id="action_withholding_tax_report_txt"
        model="withholding.tax.report"
        string="Withholding Tax Report (TXT)"
        report_type="qweb-text"
        name="l10n_th_withholding_tax_report.report_withholding_tax_text"
        file="l10n_th_withholding_tax_report.report_withholding_tax_text"
        print_report_name="'WT-%s-%s' % (object.format_pnd(object.income_tax_form), object.format_date())"
    />
    <!-- XLSX REPORTS -->
    <record id="action_withholding_tax_report_xlsx" model="ir.actions.report">
        <field name="name">Withholding Tax Report (XLSX)</field>
        <field name="model">withholding.tax.report</field>
        <field name="type">ir.actions.report</field>
        <field name="report_name">withholding.tax.report.xlsx</field>
        <field name="report_type">xlsx</field>
        <field
            name="print_report_name"
        >'WT-%s-%s' % (object.format_pnd(object.income_tax_form), object.format_date())</field>
    </record>
</odoo>
