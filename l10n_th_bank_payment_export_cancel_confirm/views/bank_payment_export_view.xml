<odoo>
    <!-- Bank Payment Export Line -->
    <record id="bank_payment_export_line_view_tree" model="ir.ui.view">
        <field name="name">bank.payment.export.line.view.tree</field>
        <field name="model">bank.payment.export.line</field>
        <field
            name="inherit_id"
            ref="l10n_th_bank_payment_export.bank_payment_export_line_view_tree"
        />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='currency_id']" position="before">
                <field name="cancel_reason" optional="show" readonly="1" />
                <field name="write_uid" optional="show" />
            </xpath>
        </field>
    </record>
</odoo>
