<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <!-- Common Template -->
    <template id="report_rd_base_company_withholding_tax">
        <div class="company_vat">
            <t t-if="o.company_id.vat">
                <div class="row">
                    <p class="box-vat">
                        <t t-out="o.company_id.vat[:1]" />
                    </p>
                    <p class="box-vat vat_tab">
                        <t t-out="o.company_id.vat[1:2]" />
                    </p>
                    <p class="box-vat">
                        <t t-out="o.company_id.vat[2:3]" />
                    </p>
                    <p class="box-vat">
                        <t t-out="o.company_id.vat[3:4]" />
                    </p>
                    <p class="box-vat">
                        <t t-out="o.company_id.vat[4:5]" />
                    </p>
                    <p class="box-vat vat_tab">
                        <t t-out="o.company_id.vat[5:6]" />
                    </p>
                    <p class="box-vat">
                        <t t-out="o.company_id.vat[6:7]" />
                    </p>
                    <p class="box-vat">
                        <t t-out="o.company_id.vat[7:8]" />
                    </p>
                    <p class="box-vat">
                        <t t-out="o.company_id.vat[8:9]" />
                    </p>
                    <p class="box-vat">
                        <t t-out="o.company_id.vat[9:10]" />
                    </p>
                    <p class="box-vat vat_tab">
                        <t t-out="o.company_id.vat[10:11]" />
                    </p>
                    <p class="box-vat">
                        <t t-out="o.company_id.vat[11:12]" />
                    </p>
                    <p class="box-vat vat_tab">
                        <t t-out="o.company_id.vat[12:13]" />
                    </p>
                </div>
            </t>
        </div>
        <div class="company_branch">
            <t t-if="o.company_id.branch">
                <div class="row">
                    <p class="box-vat">
                        <t t-out="o.company_id.branch[:1]" />
                    </p>
                    <p class="box-vat">
                        <t t-out="o.company_id.branch[1:2]" />
                    </p>
                    <p class="box-vat">
                        <t t-out="o.company_id.branch[2:3]" />
                    </p>
                    <p class="box-vat">
                        <t t-out="o.company_id.branch[3:4]" />
                    </p>
                    <p class="box-vat">
                        <t t-out="o.company_id.branch[4:5]" />
                    </p>
                </div>
            </t>
        </div>
        <span class="num_page_from" t-out="num + 1" />
        <span class="num_page_to" t-out="num_page" />
    </template>
    <template id="report_rd_base_partner_vat_withholding_tax">
        <div class="row">
            <p class="box-vat">
