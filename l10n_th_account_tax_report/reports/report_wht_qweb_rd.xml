            <p class="box-vat vat_tab">
                <t t-out="wht_data['partner_vat'][12:13]" />
            </p>
        </div>
    </template>
    <template id="report_rd_base_total_withholding_tax">
        <div class="total_base">
            <t t-out="'{0:,.2f}'.format(total_base)" />
        </div>
        <div class="total_amount">
            <t t-out="'{0:,.2f}'.format(total_amount)" />
        </div>
        <t t-if="num + 1 == num_page">
            <div class="sum_total_base">
                <t t-out="'{0:,.2f}'.format(sum_total_base)" />
            </div>
            <div class="sum_total_amount">
                <t t-out="'{0:,.2f}'.format(sum_total_amount)" />
            </div>
        </t>
    </template>

    <!-- RD PND1 -->
    <template id="report_rd_withholding_tax_pnd1">
        <t t-set="number" t-value="0" />
        <t t-set="sum_total_base" t-value="0" />
        <t t-set="sum_total_amount" t-value="0" />
        <t t-set="num_page" t-value="ceil(len(o.results)/8)" />
        <t t-foreach="range(num_page)" t-as="num">
            <!-- Header -->
            <div
                class="article o_report_rd_withholding_tax_report_pnd1"
                t-attf-style="background-image:url('/l10n_th_account_tax_report/static/src/img/{{ num + 1 == num_page and 'pnd1_lastpage.png' or 'pnd1.png' }}'); background-size:340mm 242mm; width:340mm; height:242mm;"
            >
                <t
                    t-call="l10n_th_account_tax_report.report_rd_base_company_withholding_tax"
                />
                <t t-set="total_base" t-value="0.0" />
                <t t-set="total_amount" t-value="0.0" />
                <!-- Lines -->
                <div class="wht_line">
                    <t
                        t-foreach="range(num * 8, min((num + 1) * 8, len(o.results)))"
                        t-as="i"
                    >
                        <t t-set="line" t-value="o.results[i]" />
                        <div class="row" style="width: 100%;">
                            <div class="col" style="width: 8%;">
                                <div class="row">
                                    <div class="wht_line_number">
                                        <t t-set="number" t-value="number + 1" />
                                        <span t-out="number" />
                                    </div>
                                </div>
                            </div>
                            <div class="col" style="width: 48%;">
                                <t
                                    t-set="wht_data"
                                    t-value="o._prepare_cert_data(line)"
                                />
                                <div class="row">
                                    <div class="wht_line_vat">
                                        <t t-if="wht_data['partner_vat']">
                                            <t
                                                t-call="l10n_th_account_tax_report.report_rd_base_partner_vat_withholding_tax"
                                            />
                                        </t>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="wht_line_firstname">
                                        <span t-out="wht_data['partner_firstname']" />
                                    </div>
                                    <div class="wht_line_lastname">
                                        <span t-out="wht_data['partner_lastname']" />
                                    </div>
                                </div>
                            </div>
                            <div class="col" style="width: 13%;">
                                <div class="row">
                                    <div class="wht_line_date">
                                        <span t-out="wht_data['date']" />
                                    </div>
                                </div>
                            </div>
                            <div class="col" style="width: 26%;">
                                <div class="row">
                                    <div class="wht_line_base">
                                        <t
                                            t-set="total_base"
                                            t-value="total_base + line.base"
                                        />
                                        <t t-out="'{0:,.2f}'.format(line.base)" />
                                    </div>
                                    <div class="wht_line_amount">
                                        <t
                                            t-set="total_amount"
                                            t-value="total_amount + line.amount"
                                        />
                                        <t t-out="'{0:,.2f}'.format(line.amount)" />
                                    </div>
                                </div>
                            </div>
                            <div class="col" style="width: 5%;">
                                <div class="row">
                                    <div class="wht_line_tax_payer">
                                        <span t-out="wht_data['tax_payer']" />
                                    </div>
                                </div>
                            </div>
                        </div>
                    </t>
                </div>
                <!-- Total -->
                <t t-set="sum_total_base" t-value="sum_total_base + total_base" />
                <t t-set="sum_total_amount" t-value="sum_total_amount + total_amount" />
                <t
                    t-call="l10n_th_account_tax_report.report_rd_base_total_withholding_tax"
                />
            </div>
        </t>
        <div style="page-break-before: always;" />
    </template>

