<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <!-- PDF REPORTS : Paperformat Tax -->
    <record id="paperformat_tax_report" model="report.paperformat">
        <field name="name">TAX Report A4</field>
        <field name="default" eval="True" />
        <field name="format">A4</field>
        <field name="page_height">0</field>
        <field name="page_width">0</field>
        <field name="orientation">Portrait</field>
        <field name="margin_top">24</field>
        <field name="margin_bottom">30</field>
        <field name="margin_left">7</field>
        <field name="margin_right">7</field>
        <field name="header_line" eval="False" />
        <field name="header_spacing">18</field>
        <field name="dpi">90</field>
    </record>
    <record id="paperformat_rd_tax_report" model="report.paperformat">
        <field name="name">TAX RD Report A4</field>
        <field name="default" eval="True" />
        <field name="format">A4</field>
        <field name="page_height">0</field>
        <field name="page_width">0</field>
        <field name="orientation">Landscape</field>
        <field name="margin_top">45</field>
        <field name="margin_bottom">5</field>
        <field name="margin_left">5</field>
        <field name="margin_right">5</field>
        <field name="header_line" eval="False" />
        <field name="header_spacing">40</field>
        <field name="dpi">90</field>
    </record>
    <record id="paperformat_wht_rd_report" model="report.paperformat">
        <field name="name">WHT RD Report A4</field>
        <field name="default" eval="True" />
        <field name="format">A4</field>
        <field name="page_height">0</field>
        <field name="page_width">0</field>
        <field name="orientation">Landscape</field>
        <field name="margin_top">0</field>
        <field name="margin_bottom">0</field>
        <field name="margin_left">0</field>
        <field name="margin_right">0</field>
        <field name="header_line" eval="False" />
        <field name="header_spacing">0</field>
        <field name="dpi">90</field>
    </record>
</odoo>
